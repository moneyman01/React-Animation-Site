%;\r\n    left: 50%;\r\n    transform: translate(-50%, -10%);\r\n  }\r\n  img {\r\n    width: 100%;\r\n    height: 70vh;\r\n    object-fit: cover;\r\n  }\r\n`;\r\n\r\nconst Awards = styled.div`\r\n  min-height: 80vh;\r\n  display: flex;\r\n  margin: 5rem 0rem;\r\n  align-items: center;\r\n  justify-content: space-around;\r\n  box-sizing: border-box;\r\n`;\r\n\r\nconst AwardStyle = styled.div`\r\n  padding: 5rem;\r\n  h3 {\r\n    font-size: 2rem;\r\n  }\r\n  .line {\r\n    width: 100%;\r\n    background: #23d997;\r\n    height: 0.5rem;\r\n    margin: 1rem 0rem;\r\n  }\r\n  p {\r\n    padding: 2rem 0rem;\r\n  }\r\n`;\r\nconst ImageDisplay = styled.div`\r\n  min-height: 50vh;\r\n  img {\r\n    width: 100%;\r\n    height: 100vh;\r\n    object-fit: cover;\r\n  }\r\n`;\r\n\r\nconst Award = ({ title, description }) => {\r\n  return (\r\n    <AwardStyle>\r\n      <h3>{title}</h3>\r\n      <div className=\"line\"></div>\r\n      <p>{description}</p>\r\n    </AwardStyle>\r\n  );\r\n};\r\nexport default MovieDetail;\r\n","D:\\Programming\\frontEnd\\React\\Animation Website\\react-animation-site\\src\\components\\Nav.js",[],"D:\\Programming\\frontEnd\\React\\Animation Website\\react-animation-site\\src\\components\\ServicesSection.js",[],"D:\\Programming\\frontEnd\\React\\Animation Website\\react-animation-site\\src\\components\\AboutSection.js",[],"D:\\Programming\\frontEnd\\React\\Animation Website\\react-animation-site\\src\\components\\FaqSection.js",[],"D:\\Programming\\frontEnd\\React\\Animation Website\\react-animation-site\\src\\MovieState.js",[],"D:\\Programming\\frontEnd\\React\\Animation Website\\react-animation-site\\src\\styles.js",[],"D:\\Programming\\frontEnd\\React\\Animation Website\\react-animation-site\\src\\animation.js",[],"D:\\Programming\\frontEnd\\React\\Animation Website\\react-animation-site\\src\\Wave.js",[],"D:\\Programming\\frontEnd\\React\\Animation Website\\react-animation-site\\src\\components\\Toggle.js",[],"D:\\Programming\\frontEnd\\React\\Ani